{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "env": {
        "MONGODB_URI": "@mongodb_uri",
        "MONGODB_DB_NAME": "music_recommendations",
        "SAAFY_API_BASE_URL": "https://saafy-api.vercel.app/api"
    }
}